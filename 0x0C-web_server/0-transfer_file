#!/usr/bin/env bash
# Transfers a file from our client to a server
#
# variables
if [ $# -lt 4 ]
then
  echo "Usage: 0-transfer_file PATH_TO_FILE IP USERNAME PATH_TO_SSH_KEY"
else
  scp -o StrictHostKeyChecking=no -i "$ssh-rsa  root@9fa6fe057ef8" "$root@9fa6fe057ef8:~/" "$ubuntu@$54.160.104.188":~
fi

